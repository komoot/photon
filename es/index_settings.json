{
	"analysis": {
		"analyzer": {
			"ja_index_raw": {
				"type": "custom",
				"char_filter": [
					"punctuationgreedy",
					"ja_normalize",
					"kuromoji_iteration_mark"
				],
				"tokenizer": "ja_kuromoji_tokenizer",
				"filter": [
					"word_delimiter",
					"lowercase",
					"german_normalization",
					"asciifolding",
					"unique",
					"kuromoji_baseform",
					"kuromoji_part_of_speech",
					"cjk_width",
					"ja_stop",
					"kuromoji_number",
					"kuromoji_stemmer"
				]
			},
			"ja_search_raw": {
				"type": "custom",
				"char_filter": [
					"punctuationgreedy",
					"ja_normalize",
					"kuromoji_iteration_mark"
				],
				"tokenizer": "ja_kuromoji_tokenizer",
				"filter": [
					"word_delimiter",
					"lowercase",
					"german_normalization",
					"asciifolding",
					"unique",
					"kuromoji_baseform",
					"kuromoji_part_of_speech",
					"cjk_width",
					"ja_stop",
					"kuromoji_number",
					"kuromoji_stemmer",
					"ja_hiragana_2_katakana"
				]
			},
			"index_ngram": {
				"char_filter": [
					"punctuationgreedy",
					"remove_ws_hnr_suffix",
					"ja_normalize",
					"kuromoji_iteration_mark"
				],
				"tokenizer": "edge_ngram",
				"filter": [
					"preserving_word_delimiter",
					"lowercase",
					"german_normalization",
					"asciifolding",
					"unique"
				]
			},
			"search_ngram": {
				"char_filter": [
					"punctuationgreedy",
					"ja_normalize",
					"kuromoji_iteration_mark"
				],
				"tokenizer": "standard",
				"filter": [
					"lowercase",
					"german_normalization",
					"asciifolding"
				]
			},
			"index_raw": {
				"char_filter": [
					"punctuationgreedy"
				],
				"filter": [
					"word_delimiter",
					"lowercase",
					"german_normalization",
					"asciifolding",
					"unique"
				],
				"tokenizer": "standard"
			},
			"search_raw": {
				"char_filter": [
					"punctuationgreedy"
				],
				"filter": [
					"word_delimiter",
					"lowercase",
					"german_normalization",
					"asciifolding",
					"unique"
				],
				"tokenizer": "standard"
			},
			"index_housenumber": {
				"char_filter": [
					"punctuationgreedy",
					"remove_ws_hnr_suffix"
				],
				"filter": [
					"lowercase",
					"preserving_word_delimiter"
				],
				"tokenizer": "standard"
			}
		},
		"tokenizer": {
			"edge_ngram": {
				"type": "edge_ngram",
				"min_gram": 1,
				"max_gram": 100,
				"token_chars": [
					"letter",
					"digit"
				]
			},
			"ja_kuromoji_tokenizer": {
				"mode": "search",
				"type": "kuromoji_tokenizer",
				"discard_compound_token": true,
				"user_dictionary_rules": [
				]
			},
			"ja_ngram_tokenizer": {
				"type": "ngram",
				"min_gram": 2,
				"max_gram": 3,
				"token_chars": [
					"letter",
					"digit"
				]
			}
		},
		"char_filter": {
			"punctuationgreedy": {
				"type": "pattern_replace",
				"pattern": "[\\.,']",
				"replacement": " "
			},
			"remove_ws_hnr_suffix": {
				"type": "pattern_replace",
				"pattern": "(\\d+)\\s(?=\\p{L}\\b)",
				"replacement": "$1"
			},
			"ja_normalize": {
				"type": "icu_normalizer",
				"name": "nfkc_cf",
				"mode": "compose"
			}
		},
		"filter": {
			"photonlength": {
				"min": "2",
				"type": "length"
			},
			"preserving_word_delimiter": {
				"type": "word_delimiter",
				"preserve_original": "true"
			},
			"ja_hiragana_2_katakana": {
				"type": "icu_transform",
				"id": "Hiragana-Katakana"
			}
		}
	}
}