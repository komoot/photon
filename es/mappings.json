{
  "place": {
    "dynamic": "false",
    "_all": {
      "enabled": false
    },
    "_source": {
      "excludes": ["context.*"]
    },
    "properties": {
      "osm_id": {
        "type": "long"
      },
      "osm_key": {
        "type": "string",
        "index":"not_analyzed"
      },
      "osm_type": {
        "type": "string",
        "index": "no"
      },
      "osm_value": {
        "type": "string",
        "index":"not_analyzed"
      },
      "importance": {
        "type": "float"
      },
      "housenumber": {
        "type": "string",
        "index": "not_analyzed",
        "copy_to": [
          "collector.default"
        ]
      },
      "coordinate": {
        "type": "geo_point"
      },
      "postcode": {
        "type": "string",
        "index": "no",
        "copy_to": [
          "collector.default"
        ]
      },
      "city": {
        "properties": {
          "default": {
            "type": "string",
            "index": "no",
            "copy_to": [
