            "type": "string",
            "index": "no",
            "copy_to": [
              "collector.default"
            ]
          }
        }
      },
      "country": {
        "properties": {
          "default": {
            "type": "string",
            "index": "no",
            "copy_to": [
              "collector.default"
            ]
          }
        }
      },
      "name": {
        "properties": {
          "int": {
            "type": "string",
            "index": "no",
            "fields": {
              "raw": {
                "analyzer": "index_raw",
                "type": "string"
              }
            },
            "copy_to": [
              "collector.default"
            ]
          },
          "old": {
            "type": "string",
            "index": "no",
            "fields": {
              "raw": {
                "analyzer": "index_raw",
                "type": "string"
              }
            },
            "copy_to": [
              "collector.default"
            ]
          },
          "reg": {
            "type": "string",
            "index": "no",
            "fields": {
              "raw": {
                "analyzer": "index_raw",
                "type": "string"
              }
            },
            "copy_to": [
              "collector.default"
            ]
          },
          "loc": {
            "type": "string",
            "index": "no",
            "fields": {
              "raw": {
                "analyzer": "index_raw",
                "type": "string"
              }
            },
            "copy_to": [
              "collector.default"
            ]
          },
          "alt": {
            "type": "string",
            "index": "no",
            "fields": {
              "raw": {
                "analyzer": "index_raw",
                "type": "string"
              }
            },
            "copy_to": [
              "collector.default"
